# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# Firebase Development Features
VITE_USE_FIREBASE_EMULATORS=false
VITE_ENABLE_ANALYTICS=false

# Application Settings
VITE_APP_NAME="Fezz Wholesale Platform"
VITE_APP_DESCRIPTION="Premium Wholesale Platform for Nicotine Pouches"
VITE_APP_URL=https://your-app-domain.com
VITE_CONTACT_EMAIL=contact@your-app-domain.com

# Feature Flags
VITE_ENABLE_PAYMENT_GATEWAY=true
VITE_ENABLE_DISTRIBUTOR_FEATURES=true
VITE_ENABLE_COMMISSION_SYSTEM=true